{
  "name": "csci.club",
  "version": "1.0.0",
  "description": "Main website for the St Cloud State University Computer Science Club and ACM Chapter",
  "main": "server/index.js",
  "scripts": {
    "build": "npm run lint && rm -rf server_dist && babel server -d server_dist && webpack",
    "debug": "NODE_ENV=debug && cd server && nodemon index.js --exec babel-node",
    "dev": "npm debug",
    "lint": "eslint --ignore-path .eslintignore server client",
    "postinstall": "npm run build",
    "start": "NODE_ENV=production && cd server_dist && node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CSCIClub/csci.club.git"
  },
  "author": "Jared Rickert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CSCIClub/csci.club/issues"
  },
  "homepage": "https://github.com/CSCIClub/csci.club#readme",
  "engines": {
    "node": "7.7.2",
    "npm": "4.4.1"
  },
  "dependencies": {
    "Particleground.js": "^1.1.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.3",
    "url-loader": "^0.5.8",
    "webpack-merge": "^4.0.0"
  }
}
